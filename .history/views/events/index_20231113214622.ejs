<h2>Events Table</h2>
<table class="table">
  <thead>
    <tr>
      <th>Tile</th>
      <th>Company</th>
      <th>Location</th>
      <th>Description</th>
      <th>Requirements</th>
      <th>Salary</th>
      <th>Contact Email</th>
      <th>Contact Phone</th>
      <th>Post Date</th>
      <th>Deadline Date</th>
      <th>Is Active</th>
    </tr>
  </thead>
  <tbody>
    <% events.forEach(event => { %>
    <tr>
      <td>
        <a href="<%= `/events/${event._id}` %>"> <%= event.title %> </a>
      </td>
      <td><%= event.company %></td>
      <td><%= event.location %></td>
      <td><%= event.description %></td>
      <td><%= event.requirements %></td>
      <td><%= event.salary %></td>
      <td><%= event.contactEmail %></td>
      <td><%= event.contactPhone %></td>
      <td><%= event.postDate %></td>
      <td><%= event.deadlineDate %></td>
      <td><%= event.isActive %></td>
      <td>
        <a href="<%=`/events/${event._id}/edit` %>"> Edit </a>
      </td>
      <td>
        <a
          href="<%= `events/${event._id}/delete?_method=DELETE` %>"
          onclick="return confirm('Are you sure you want to delete this record?')"
          >Delete</a
        >
      </td>
    </tr>
    <% }); %>
  </tbody>
</table>
